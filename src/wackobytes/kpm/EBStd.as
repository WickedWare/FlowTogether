package com.kpm.kpm{			import com.kpm.util.Util;		public class EBStd	{			private var index : uint ;	  	private var weight : uint;        private var active : Boolean;		public var bubbleNames : Array;	    public static const Numbers_Count		 :EBStd = new EBStd(0,5);		public static const Numbers_Identify	 :EBStd = new EBStd(1,7);		public static const Numbers_Subset		 :EBStd = new EBStd(2,4);	    public static const Geometry	   		 :EBStd = new EBStd(3,3);	    public static const SpatialSense	   	 :EBStd = new EBStd(4,8);	    public static const DataMeasurements	 :EBStd = new EBStd(5,5);   	    public static const Addition	 		 :EBStd = new EBStd(6,3);   	    public static const Comparison	 		 :EBStd = new EBStd(7,3);        public static const PlaceNumber	 		 :EBStd = new EBStd(8,4);	    public var Text :String;	    {Util.initEnumConstants(EBStd);} // static ctor	    	    private static var constList : Array = 		Util.getConstantsInArray(EBStd);	    	    public function get Index() : uint 				{ return index; }					public static function get Consts() : Array 	{ return constList};			    	    public function EBStd (pIndex : uint, pWeight : int)	    {	    	index = pIndex;            weight = pWeight;	    }	    	    public function get ReadableText() : String	    {	    	var splitArray : Array = Text.split("_");	    	return splitArray[splitArray.length-1];	    }	    	    public function get Weight() : uint	    {	    	return weight;	    }	    	    public function set Weight(pWeight : uint) 	    {	    	weight = pWeight;	    }        public function get Active() : Boolean        {            return active;        }        public function set Active(pActive : Boolean)        {            active = pActive;        }		public function equals(pStd : Object) : Boolean		{			if(pStd is EBName)				return (pStd.Standard.Text == this.Text);						return (pStd.Text == this.Text)		}	}}
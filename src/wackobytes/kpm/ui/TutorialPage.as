package com.kpm.kpm{	import com.kpm.util.KpmIO;	import com.kpm.util.MovieList;	import com.kpm.util.Point2D;	import com.kpm.util.Util;	import com.kpm.reporter.excel.Kid;	import com.kpm.ui.UIConst;	import com.kpm.ui.UIGod;	import com.kpm.ui.UIPage;		import fl.controls.RadioButton;	import fl.controls.RadioButtonGroup;		import flash.display.MovieClip;	import flash.events.Event;	import flash.events.MouseEvent;	import flash.net.URLRequest;	import flash.net.navigateToURL;	import flash.text.TextField;	import flash.text.TextFieldAutoSize;	import flash.text.TextFormat;			public class TutorialPage extends UIPage	{		var buttons : Array ;		var numRows : int = 3;		var numButtons : int = 8;		var buttons_initialPos : Point2D = new Point2D(20, 20);				public function TutorialPage()		{			for(var i=0; i < numButtons; i++)			{				buttons.push(Util.addButton("TutoriallessonBt", this, i%2, i/2, onLessonClick, i));				buttons[i].x += buttons_initialPos.x;				buttons[i].y += buttons_initialPos.x;								buttons[i].tLessonNumber.Text =  i;								}			  			super(UIConst.TutorialPage);		}				public override function showPage(e : Event = null)		{							super.showPage();		}				}}